{
	"name": "sprits",
	"module": "./dist/es/index.mjs",
	"main": "./dist/cjs/index.js",
	"types": "./dist/cjs/index.d.ts",
	"version": "0.0.2",
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@changesets/cli": "^2.27.9",
		"@types/node": "^22.8.1",
		"bunchee": "^5.5.1",
		"effect": "^3.10.4",
		"ts-graphviz": "^2.1.4",
		"vitest": "^2.1.3"
	},
	"peerDependencies": {
		"effect": "^3.10.4",
		"ts-graphviz": "^2.1.4",
		"typescript": "^5.0.0"
	},
	"scripts": {
		"build": "bunchee",
		"changeset:publish": "pnpm run build && changeset publish",
		"changeset:version": "changeset version && pnpm install --no-frozen-lockfile",
		"lint": "biome ci",
		"prepare": "bunchee --prepare && biome format --fix package.json",
		"test": "vitest"
	},
	"packageManager": "pnpm@9.12.1+sha512.e5a7e52a4183a02d5931057f7a0dbff9d5e9ce3161e33fa68ae392125b79282a8a8a470a51dfc8a0ed86221442eb2fb57019b0990ed24fab519bf0e1bc5ccfc4",
	"files": ["dist"],
	"exports": {
		".": {
			"import": {
				"types": "./dist/es/index.d.mts",
				"default": "./dist/es/index.mjs"
			},
			"require": {
				"types": "./dist/cjs/index.d.ts",
				"default": "./dist/cjs/index.js"
			}
		},
		"./step": {
			"import": {
				"types": "./dist/es/step.d.mts",
				"default": "./dist/es/step.mjs"
			},
			"require": {
				"types": "./dist/cjs/step.d.ts",
				"default": "./dist/cjs/step.js"
			}
		}
	}
}
